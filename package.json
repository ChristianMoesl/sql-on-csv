{
  "name": "wasmdb",
  "version": "1.0.0",
  "description": "",
  "main": "bundle.js",
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "printf": "^0.5.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "streamsaver": "^2.0.3",
    "web-streams-polyfill": "2.0.4",
    "webworkify": "^1.5.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-window": "^1.8.1",
    "autoprefixer": "^9.6.1",
    "browserify": "^16.5.0",
    "envify": "^4.1.0",
    "node-sass": "^4.12.0",
    "npm-watch": "^0.6.0",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "terser": "^4.3.1",
    "tsify": "^4.0.1",
    "typescript": "^3.6.3",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  },
  "scripts": {
    "test": "echo \"no test specified\" && exit 0",
    "build": "npm run build:ts && npm run build:scss",
    "build-prod": "npm run build:ts-prod && npm run build:scss-prod",
    "watch": "npm-watch build",
    "build:ts": "browserify app/scripts/app.tsx -p [ tsify --noImplicitAny ] > dist/bundle.js",
    "build:ts-prod": "NODE_ENV=production browserify app/scripts/app.tsx -p tsify -t [ envify --NODE_ENV production ] -t uglifyify | terser --compress --mangle > dist/bundle.js",
    "build:scss": "node-sass app/styles/styles.scss -o dist && postcss -u autoprefixer -r dist/styles.css",
    "build:scss-prod": "node-sass app/styles/styles.scss -o dist --style compressed && postcss -u autoprefixer -r dist/styles.css"
  },
  "watch": {
    "build": {
      "patterns": [
        "app"
      ],
      "extensions": "tsx,ts,html,scss",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristianMoesl/wasmdb.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChristianMoesl/wasmdb/issues"
  },
  "homepage": "https://github.com/ChristianMoesl/wasmdb#readme"
}
